<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>{{app.Settings.sitename}}</title>
    <link href="/dist/css/app.css" rel="stylesheet" type="text/css" >
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Material+Icons|Material+Icons+Outlined|Material+Icons+Two+Tone|Material+Icons+Round|Material+Icons+Sharp|Cairo:wght@300;500;700;900">
    <script type="text/javascript">
        var enable_debug = {{app.Settings.enable_debug ? 1 : 0}};
    </script>
</head>

<body style="direction: {{lang.__('dir')}};" class="{{lang.__('dir')}}">
    <div class="bg-white rounded-lg shadow-lg overflow-hidden">
  <div class="px-6 py-4">
    <h2 class="text-lg font-bold mb-2">Customer Profile</h2>
    <p class="text-gray-600">Booking History</p>
  </div>
  <div class="border-t border-gray-200">
    <ul class="divide-y divide-gray-200">
      <li class="px-6 py-4">
        <div class="flex items-center">
          <div class="flex-shrink-0">
            <img class="h-10 w-10 rounded-full" src="https://via.placeholder.com/150" alt="">
          </div>
          <div class="ml-4">
            <p class="text-sm font-medium text-gray-900">Booking #12345</p>
            <p class="text-sm text-gray-500">Flight to New York</p>
            <p class="text-sm text-gray-500">Departure Date: Jun 1, 2023</p>
            <p class="text-sm text-gray-500">Booking Date: May 1, 2023</p>
          </div>
        </div>
      </li>
      <li class="px-6 py-4">
        <div class="flex items-center">
          <div class="flex-shrink-0">
            <img class="h-10 w-10 rounded-full" src="https://via.placeholder.com/150" alt="">
          </div>
          <div class="ml-4">
            <p class="text-sm font-medium text-gray-900">Booking #67890</p>
            <p class="text-sm text-gray-500">Hotel in Paris</p>
            <p class="text-sm text-gray-500">Check-in Date: May 15, 2023</p>
            <p class="text-sm text-gray-500">Booking Date: May 8, 2023</p>
          </div>
        </div>
      </li>
      <li class="px-6 py-4">
        <div class="flex items-center">
          <div class="flex-shrink-0">
            <img class="h-10 w-10 rounded-full" src="https://via.placeholder.com/150" alt="">
          </div>
          <div class="ml-4">
            <p class="text-sm font-medium text-gray-900">Booking #24680</p>
            <p class="text-sm text-gray-500">Car Rental in Los Angeles</p>
            <p class="text-sm text-gray-500">Pick-up Date: Jul 1, 2023</p>
            <p class="text-sm text-gray-500">Booking Date: Apr 20, 2023</p>
          </div>
        </div>
      </li>
    </ul>
  </div>
</div>
    <div id="app">
    {% include 'views/admin/includes/nodata.html.twig' %}    
    </div>

    <div>            
        <div id="root-parent"  
        data-status="{{app.request().get('load')}}"
        data-page="{{app.request().requestUri|slice(1)}}"
        data-menu="{{app.menu()|json_encode}}"
        data-auth="{{app.auth|json_encode}}"
        data-lang="{{lang.vueLang()|json_encode}}"
        data-setting="{{app.Settings|json_encode}}"
        data-conf="{{app.CONF|json_encode}}"
        data-types-list="{{typesList|json_encode}}"
        ></div>

    </div>
                 
    <style type="text/css">
        .text-default{ text-align:  {{app.Settings.lang == 'arabic' ? 'right' : 'left'}} }
    </style>

    <script type="text/javascript" src="/dist/js/app.js"></script>
    <script src="https://html2canvas.hertzen.com/dist/html2canvas.min.js"></script>
</body>
</html>
