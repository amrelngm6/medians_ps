<!DOCTYPE html>
<html lang="en" class="light">
<!-- BEGIN: Head -->

<head>
    <meta charset="utf-8">
    <link href="dist/images/logo.svg" rel="shortcut icon">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Enigma admin is super flexible, powerful, clean & modern responsive tailwind admin template with unlimited possibilities.">
    <meta name="keywords" content="admin template, Enigma Admin Template, dashboard template, flat admin template, responsive admin template, web app">
    <meta name="author" content="LEFT4CODE">
    <title>Dashboard - Enigma - Tailwind HTML Admin Template</title>
    <!-- BEGIN: CSS Assets-->
    <link href="/dist/css/app.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Material+Icons|Material+Icons+Outlined|Material+Icons+Two+Tone|Material+Icons+Round|Material+Icons+Sharp|Tajawal|Cairo:wght@300;500;700;900">
    <script type="text/javascript">
    var enable_debug = {{ app.Settings.enable_debug ? 1 : '0' }};
    </script>

    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <!-- END: CSS Assets-->
</head>
<!-- END: Head -->

<body style="direction: {{lang.__('dir')}};" class="{{lang.__('dir')}}">
    
    
    <div id="app">
        <div class="text-center mx-auto" style="text-align: center; max-width: 800px; margin: 0 auto;">
            <img class="mx-auto" src="/uploads/img/no-data.png" />
        </div>
    </div>
    <div>
        <div id="root-parent" data-status="{{app.request().get('load')}}" data-page="{{app.request().requestUri|slice(1)}}" data-menu="{{app.menu()|json_encode}}" data-auth="{{app.auth|json_encode}}" data-lang="{{lang.vueLang()|json_encode}}" data-setting="{{app.Settings|json_encode}}" data-system_setting="{{app.SystemSetting|json_encode}}" data-conf="{{app.CONF|json_encode}}" data-active-tab="{{component}}" data-component="{{component}}" data-types-list="{{typesList|json_encode}}"></div>
    </div>
    <style type="text/css">
    body .text-default {
        text-align: {
                {
                app.Settings.lang=='arabic'? 'right': 'left'
            }
        }
    }
    </style>
    <script type="text/javascript" src="/dist/js/app.js"></script>
    <script src="https://html2canvas.hertzen.com/dist/html2canvas.min.js"></script>
</body>

</html>