        "nikic/fast-route": "^1.3",
        "symfony/mime": "^6.0",
        "phpseclib/phpseclib": "~3.0",
        "phpmailer/phpmailer": "^6.8",
        "google/apiclient": "^2.15",
        "omnipay/paypal": "*",
